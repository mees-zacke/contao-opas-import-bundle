{
    "name": "floxn/contao-opas-import-bundle",
    "type": "contao-bundle",
    "description": "Extension to import data from OPAS via Uploaded XML file to /files/xml/export.xml",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Florian Krohmer",
            "homepage": "https://github.com/Floxn"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "contao/core-bundle": "~4.4",
        "marebe1602/mae_event_categories": "^1.5"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Floxn\\ContaoOpasImportBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Floxn\\ContaoOpasImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floxn\\ContaoOpasImportBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/floxn/contao-opas-import-bundle/issues",
        "source": "https://github.com/floxn/contao-opas-import-bundle"
    }
}
